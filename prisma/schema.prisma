generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./database.sqlite"
}

model Courses {
  id          String   @id @default(uuid())
  name        String   @unique
  description String?
  duration    Int
  created_at  DateTime @default(now())

  @@map("courses")
}
